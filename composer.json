{
  "name": "davidhirtz/yii2-media-video",
  "type": "yii2-extension",
  "description": "Yii2 video extension for yii2-media module",
  "homepage": "https://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "davidhirtz/yii2-media": "^2.0",
    "james-heinrich/getid3": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\media\\video\\": "src"
    }
  },
  "extra": {
    "bootstrap": "davidhirtz\\yii2\\media\\video\\composer\\Bootstrap"
  }
}